{
  "name": "foobar",
  "type": "module",
  "imports": { "#*": "./*.js" },
 "scripts": {
    "start": "node server.js",               
    "build": "npm --prefix ems-ui ci && npm --prefix ems-ui run build",

    "dev": "node --watch --env-file=.env server.js", 
    "db:schema": "psql \"$DATABASE_URL\" -f db/schema.sql",
    "db:seed": "node --env-file=.env db/seed.js",
    "db:reset": "npm run db:schema && npm run db:seed",
    "test": "vitest"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "react-router-dom": "^7.9.1"
  },
  "engines": { "node": ">=22.0.0" },
  "devDependencies": { "vitest": "^3.1.3" }
}
