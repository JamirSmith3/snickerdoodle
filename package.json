{
  "name": "foobar",
  "type": "module",
  "imports": {
    "#*": "./*.js",
    "#db/*": "./db/*",
    "#api/*": "./api/*",
    "#middleware/*": "./middleware/*"
  },
  "scripts": {
    "db:migrate": "psql \"$DATABASE_URL\" -f db/schema.sql",
    "db:seed": "node db/seed.js",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "start": "node server.js",
    "dev": "node --watch --env-file=.env server.js",
    "test": "vitest",
    "db:schema": "psql -d foobar -f db/schema.sql"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "react-router-dom": "^7.9.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "vitest": "^3.1.3"
  }
}